{
  "version": 2,
  "name": "mindlift-frontend",
  "builds": [
    { "src": "frontend/package.json", "use": "@vercel/static-build", "config": { "distDir": "build" } }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "frontend/$1" }
  ],
  "env": {
    "REACT_APP_API_URL": "https://mindlift-backend-production.up.railway.app"
  }
}
